{
    "routes": [
        {
            "route": "/login",
            "redirect": ".auth/login/github"
        },
        {
            "route": "/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/github"
        }
    }
}